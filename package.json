{
  "name": "genarogg-pg2-alberto",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "develop": "nodemon ./bin/www",
    "scss": "sass --watch public/css/sass/style.scss public/css/style.css",
    "browser-sync": "browser-sync start --server --serveStatic html",
    "dev": "concurrently \"npm run develop\" \"npm run browser-sync\" \"npm run scss\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "sass": "^1.62.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
}
